# This file allows users to call find_package(SHORTNAIL) and pick up our targets.

@SHORTNAIL_CONFIG_CODE@

#find_package(MLIR REQUIRED CONFIG)
find_package(CIRCT REQUIRED CONFIG)

set(SHORTNAIL_EXPORTED_TARGETS "@SHORTNAIL_EXPORTS@")
set(SHORTNAIL_CMAKE_DIR "@SHORTNAIL_CONFIG_CMAKE_DIR@")
set(SHORTNAIL_BINARY_DIR "@SHORTNAIL_CONFIG_BINARY_DIR@")
set(SHORTNAIL_INCLUDE_DIRS "@SHORTNAIL_CONFIG_INCLUDE_DIRS@")
set(SHORTNAIL_LIBRARY_DIRS "@SHORTNAIL_CONFIG_LIBRARY_DIRS@")
set(SHORTNAIL_TOOLS_DIR "@SHORTNAIL_CONFIG_TOOLS_DIR@")

set_property(GLOBAL PROPERTY SHORTNAIL_ALL_LIBS "@SHORTNAIL_ALL_LIBS@")
set_property(GLOBAL PROPERTY SHORTNAIL_DIALECT_LIBS "@SHORTNAIL_DIALECT_LIBS@")
set_property(GLOBAL PROPERTY SHORTNAIL_CONVERSION_LIBS "@SHORTNAIL_CONVERSION_LIBS@")

# Provide all our library targets to users.
@SHORTNAIL_CONFIG_INCLUDE_EXPORTS@
